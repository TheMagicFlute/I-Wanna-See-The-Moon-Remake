    objWorld.__ONLINE_name = string_copy(objWorld.__ONLINE_name, 0, 20);
objWorld.__ONLINE_password = get_string("Enter a password (max 20 chars):", "123");
if (string_length(objWorld.__ONLINE_password) &gt; 20)
    objWorld.__ONLINE_password = string_copy(objWorld.__ONLINE_password, 0, 20);
objWorld.__ONLINE_selfGameID += objWorld.__ONLINE_password;
objWorld.__ONLINE_team = get_string("Which team do you want to be in? (1~9)", "1");
objWorld.__ONLINE_name = "team" + objWorld.__ONLINE_team + "@" + objWorld.__ONLINE_name;
//objWorld.__ONLINE_server = get_string("Which server do you want to connect to", "iwannastartaparty.com");   
objWorld.__ONLINE_server = "iwannastartaparty.com"; 

switch (objWorld.__ONLINE_team)
{
    case "1":
        objWorld.__ONLINE_tea = 1;
        break;
    case "2":
        objWorld.__ONLINE_tea = 2;
        break;
    case "3":
        objWorld.__ONLINE_tea = 3;
        break;
    case "4":
        objWorld.__ONLINE_tea = 4;
        break;
    case "5":
        objWorld.__ONLINE_tea = 5;
